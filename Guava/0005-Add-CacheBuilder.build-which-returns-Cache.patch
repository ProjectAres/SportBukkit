From 290921b320b64c9f90dc362c50b9e31212853776 Mon Sep 17 00:00:00 2001
From: Marcos Vives Del Sol <socram8888@gmail.com>
Date: Thu, 24 Apr 2014 15:45:07 +0200
Subject: [PATCH] Add CacheBuilder.build which returns Cache


diff --git a/guava/maps.yml b/guava/maps.yml
index 6cb7ae2..f71b05e 100644
--- a/guava/maps.yml
+++ b/guava/maps.yml
@@ -1,2 +1,3 @@
 members:
   "com/google/common/cache/CacheBuilder _INVALID_maximumSize (I)Lcom/google/common/cache/CacheBuilder;": maximumSize
+  "com/google/common/cache/CacheBuilder _INVALID_build (Lcom/google/common/cache/CacheLoader;)Lcom/google/common/cache/Cache;": build
diff --git a/guava/src/com/google/common/cache/CacheBuilder.java b/guava/src/com/google/common/cache/CacheBuilder.java
index f56c8f1..3161550 100644
--- a/guava/src/com/google/common/cache/CacheBuilder.java
+++ b/guava/src/com/google/common/cache/CacheBuilder.java
@@ -794,6 +794,12 @@ public final class CacheBuilder<K, V> {
     return new LocalCache.LocalLoadingCache<K1, V1>(this, loader);
   }
 
+  @Deprecated
+  public <K1 extends K, V1 extends V> Cache<K1, V1> _INVALID_build(
+      CacheLoader<? super K1, V1> loader) {
+    return build(loader);
+  }
+
   /**
    * Builds a cache which does not automatically load values when keys are requested.
    *
-- 
1.9.1

