From 74d2674bbe802902481e9b0e72b7739ec03c505c Mon Sep 17 00:00:00 2001
From: YukonAppleGeek <yukonvinecki@gmail.com>
Date: Wed, 3 Apr 2013 17:40:25 -0700
Subject: [PATCH 1329/1329] Add getEntity to BlockDispenseEvent

---
 .../java/org/bukkit/event/block/BlockDispenseEvent.java  |   14 +++++++++++++-
 1 file changed, 13 insertions(+), 1 deletion(-)

diff --git a/src/main/java/org/bukkit/event/block/BlockDispenseEvent.java b/src/main/java/org/bukkit/event/block/BlockDispenseEvent.java
index 5d01beb..f62fec3 100644
--- a/src/main/java/org/bukkit/event/block/BlockDispenseEvent.java
+++ b/src/main/java/org/bukkit/event/block/BlockDispenseEvent.java
@@ -1,6 +1,7 @@
 package org.bukkit.event.block;
 
 import org.bukkit.block.Block;
+import org.bukkit.entity.Entity;
 import org.bukkit.event.Cancellable;
 import org.bukkit.event.HandlerList;
 import org.bukkit.inventory.ItemStack;
@@ -16,11 +17,13 @@ public class BlockDispenseEvent extends BlockEvent implements Cancellable {
     private boolean cancelled = false;
     private ItemStack item;
     private Vector velocity;
+    private Entity entity;
 
-    public BlockDispenseEvent(final Block block, final ItemStack dispensed, final Vector velocity) {
+    public BlockDispenseEvent(final Block block, final ItemStack dispensed, final Vector velocity, final Entity entity) {
         super(block);
         this.item = dispensed;
         this.velocity = velocity;
+        this.entity = entity;
     }
 
     /**
@@ -62,6 +65,15 @@ public class BlockDispenseEvent extends BlockEvent implements Cancellable {
         velocity = vel;
     }
 
+    /**
+     * Gets the entity that is being dispensed.
+     *
+     * @return An Entity for the item being dispensed
+     */
+    public Entity getEntity() {
+        return entity;
+    }
+
     public boolean isCancelled() {
         return cancelled;
     }
